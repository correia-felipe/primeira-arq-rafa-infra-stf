{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "StartGlueJobs",
      "Effect": "Allow",
      "Action": [ "glue:StartJobRun", "glue:GetJobRun" ],
      "Resource": "*"
    },
    {
      "Sid": "Logs",
      "Effect": "Allow",
      "Action": [ "logs:CreateLogGroup", "logs:CreateLogStream", "logs:PutLogEvents" ],
      "Resource": "*"
    }
  ]
}
